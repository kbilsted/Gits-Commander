{
    "Console": {
        "IncludeScopes": true,
        "LogLevel": {
            "Default": "Error",
            "Microsoft": "Error",
            "System": "Error"
        }
    },
    "Configuration": {
        "GitExePath": "C:\\Program Files\\Git\\bin\\git.exe",
        "RepoListLocalCachePath": "C:\\temp\\gitscommander.state.json",
        "Launchers": {
            "F1": {
                "Name": "Open Git gui",
                "Command": "C:\\Program Files\\GitExtensions\\gitextensions.exe",
                "Arguments": [ "browse" ]
            },
            "F4": {
                "Name": "Open code (Visual studio)",
                "Command": "$file$",
                "FilePattern": "*.sln"
            },
            "F5": {
                "Name": "Refresh",
                "Command": "$refreshRepoList$"
            },
            "F8": {
                "Name": "Open folder",
                "Command": "$folder$"
            },
            "F9": {
                "Name": "Edit configuration",
                "Command": "$openAppSettings$"
            }
        },
        "RepositorySources": {
        }
    }
}